#!/bin/zsh

cd ..

#              "--whnfApplySubst" \
#              "--whnfEliminate" \
#              "--disableSynEquality --whnfApplySubst" \
#              "--disableSynEquality --whnfEliminate" \
#              "--disableSynEquality" \
#              "--whnfApplySubst --whnfEliminate" \
#              "--disableSynEquality --whnfApplySubst --whnfEliminate" \

for dir in working shouldfail slow; do
  for f in examples/$dir/*.agda; do
      for term_type in S GR GRU H; do
          for whnf in "" \
               "--disableSynEquality" \
               "--physicalEquality" \
               "--physicalEquality --disableSynEquality" \
              ; do
              echo bm_tog $f --termType $term_type $whnf
          done
      done
      for sharing_type in --sharing --no-sharing; do
          echo bm_agda $f $sharing_type
      done   
  done
done
